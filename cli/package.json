{
	"name": "@rp1-run/rp1",
	"version": "0.2.9",
	"description": "AI-assisted development workflows CLI",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/rp1-run/rp1.git",
		"directory": "cli"
	},
	"homepage": "https://rp1.run",
	"bugs": {
		"url": "https://github.com/rp1-run/rp1/issues"
	},
	"keywords": [
		"ai",
		"cli",
		"development",
		"workflows",
		"claude",
		"opencode",
		"automation"
	],
	"type": "module",
	"bin": {
		"rp1": "./dist/main.js"
	},
	"files": [
		"dist",
		"shared",
		"web-ui/dist",
		"web-ui/src",
		"web-ui/package.json"
	],
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"postinstall": "bun run scripts/postinstall.ts",
		"build": "bun build src/main.ts --outdir dist --target node",
		"build:web-ui": "cd web-ui && bun run build",
		"build:opencode": "bun run scripts/build-opencode.ts",
		"build:all": "bun run build && bun run build:web-ui && bun run build:opencode",
		"generate:assets": "bun run scripts/generate-asset-imports.ts",
		"build:release": "bun run build:web-ui && bun run build:opencode && bun run generate:assets",
		"dev": "bun run src/main.ts",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"test": "bun test",
		"test:unit": "find src/__tests__ -name '*.test.ts' ! -path '*integration*' -print0 | xargs -0 bun test",
		"test:integration": "find src/__tests__ -name '*.test.ts' -path '*integration*' -print0 | xargs -0 bun test",
		"test:coverage": "bun test --coverage",
		"test:watch": "bun test --watch",
		"prepublishOnly": "bun run build:all"
	},
	"dependencies": {
		"@inquirer/prompts": "^8.0.2",
		"chalk": "^5.6.2",
		"commander": "^14.0.2",
		"consola": "^3.4.2",
		"figures": "^6.1.0",
		"fp-ts": "^2.16.9",
		"ink": "^5.0.1",
		"ink-spinner": "^5.0.0",
		"nanoid": "^5.0.9",
		"ora": "^9.0.0",
		"puppeteer": "^24.34.0",
		"react": "^18.3.1",
		"react-devtools-core": "^6.1.1",
		"yaml": "^2.3.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@types/bun": "^1.1.14",
		"@types/node": "^22.0.0",
		"@types/react": "^18.3.0",
		"ink-testing-library": "^4.0.0",
		"typescript": "^5.7.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
